{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "windsurf-account-manager",
  "version": "1.6.7",
  "identifier": "com.chao.windsurf-account-manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:46952",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "windsurf-account-manager-simple",
        "width": 1200,
        "height": 800,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      },
      "wix": {
        "language": [
          "en-US",
          "zh-CN",
          "tr-TR"
        ]
      },
      "nsis": {
        "languages": [
          "English",
          "SimpChinese",
          "Turkish"
        ],
        "displayLanguageSelector": true
      }
    },
    "resources": {
      "windsurf-account-manager.exe.manifest": "",
      "cunzhi-bundle/*": "cunzhi/"
    }
  }
}